LEVEL := ../

COMMON_WARNINGS := \
	-Wno-uninitialized \
	-Wno-return-type \

# Necessary to avoid the "declaration does not declare anything" error
GCC_WARNINGS := -Wno-error \

CLANG_WARNINGS := \
	-Wno-incompatible-library-redeclaration \
	-Wno-gnu-variable-sized-type-not-at-end \
	-Wno-missing-declarations \
	-Wno-tautological-compare \

include $(LEVEL)/Makefile.config
